{
    "routes": [
      {
        "src": "/ads.txt",
        "dest": "/ads.txt"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/.*\\.(js|css|json|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$",
        "dest": "/$0"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }